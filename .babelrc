{
  "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
  "plugins": ["@babel/plugin-proposal-class-properties", [
    "babel-plugin-root-import",
    {
      "paths": [{
        "rootPathPrefix": "~/",
        "rootPathSuffix": "./assets"
      }, {
        "rootPathPrefix": "/",
        "rootPathSuffix": "./"
      }]
    }
  ]]
}
